<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>encoding</param-name>
        <param-value>UTF-8</param-value>
    </context-param>
    <jsp-config>
        <taglib>
            <taglib-uri>http://wwww.shubaoz.top/myfunction</taglib-uri>
            <taglib-location>/WEB-INF/myfun.tld</taglib-location>
        </taglib>
    </jsp-config>
    <servlet>
        <servlet-name>my</servlet-name>
        <servlet-class>com.bjsxt.servlet.MyServlet</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.bjsxt.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>TestGetPostServlet</servlet-name>
        <servlet-class>com.bjsxt.servlet.TestGetPostServlet</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>TestRequest</servlet-name>
        <servlet-class>com.bjsxt.servlet.TestRequest</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>ReqEncoding</servlet-name>
        <servlet-class>com.bjsxt.servlet.ReqEncoding</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>TestResponse</servlet-name>
        <servlet-class>com.bjsxt.servlet.TestResponse</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>AServlet</servlet-name>
        <servlet-class>com.bjsxt.forward.AServlet</servlet-class>
    </servlet>
    <servlet>
        <description>This is the description of my J2EE component</description>
        <display-name>This is the display name of my J2EE component</display-name>
        <servlet-name>BServlet</servlet-name>
        <servlet-class>com.bjsxt.forward.BServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>my</servlet-name>
        <url-pattern>/s</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestGetPostServlet</servlet-name>
        <url-pattern>/gp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestRequest</servlet-name>
        <url-pattern>/re</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReqEncoding</servlet-name>
        <url-pattern>/rec</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestResponse</servlet-name>
        <url-pattern>/resp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AServlet</servlet-name>
        <url-pattern>/a</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BServlet</servlet-name>
        <url-pattern>/b</url-pattern>
    </servlet-mapping>
</web-app>